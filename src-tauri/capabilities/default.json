{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    "fs:allow-app-write",
    "fs:allow-app-read",
    "fs:allow-resource-read-recursive",
    {
      "identifier": "fs:allow-exists",
      "allow": [
        {
          "path": "$APPDATA/*"
        },
        {
          "path": "$RESOURCE/**"
        }
      ]
    },
    "dialog:default"
  ]
}